{
  "languageserver":{
  "golang": {
    "command": "gopls",
    "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
    "disableWorkspaceFolders": true,
    "filetypes": ["go"]
  },
  "sql": {
    "command": "sql-language-server",
    "args" : ["up", "--method", "stdio"],
    "filetypes": ["sql", "mysql"]
    }
},
 "snippets.ultisnips.directories":
  [
    "UltiSnips",
    "~/.config/nvim/utils/snips"
  ]
}

